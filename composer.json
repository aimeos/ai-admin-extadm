{
	"name": "aimeos/ai-admin-extadm",
	"description": "Aimeos ai-admin-extadm extension",
	"keywords": ["aimeos", "extension", "ExtJS admin interface"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-admin-extadm",
		"issues": "https://github.com/aimeos/ai-admin-extadm/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "^1.0",
		"aimeos/aimeos-core": "2016.07.*"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master",
		"squizlabs/php_codesniffer": "~1.0",
		"phpunit/phpunit": "~3.7||~4.0||~5.0",
		"phing/phing": "~2.4"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\Controller\\ExtJS\\": "controller/extjs/src/Controller/ExtJS/"
		},
		"classmap": [
			"controller/extjs/src"
		]
	}
}
